#include <iostream>
#include <stdarg.h>
using namespace std;

int triangle(int m, ...)
{
    int a, b, c;
    int s = 0;
    int* ptr = &m + 1;
    for (; m > 0; m--)
    {
        a = *(ptr + 1);
        b = *(ptr + 3);
        c = *(ptr + 5);

        if (a * a + b * b >= c * c)
        {
            if (a * a + c * c >= b * b)
            {
                if (b * b + c * c >= a * a)
                {
                    s++;
                }
            }
        }

        ptr = ptr + 2;
    }
    return s;
}

